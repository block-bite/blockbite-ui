{
  "name": "@blockbite/ui",
  "version": "1.0.0",
  "description": "Shared React UI components for Blockbite apps",
  "author": "Blockbite",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run types",
    "preview": "vite preview",
    "lint": "eslint ./src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "types": "tsc --project tsconfig.build.json"
  },
  "peerDependencies": {
    "@blockbite/icons": "^4.4.7",
    "@wordpress/api-fetch": "^6.21.0",
    "@wordpress/block-editor": "^14.16.0",
    "@wordpress/components": "^29.7.0",
    "@wordpress/element": "^6.21.0",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.16",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.0.11",
    "@types/wordpress__block-editor": "^11.5.16",
    "@types/wordpress__blocks": "^12.5.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.1",
    "typescript": "^5.8.2",
    "vite": "^5.0.0"
  },
  "keywords": [
    "ui",
    "react",
    "components",
    "blockbite",
    "vite",
    "typescript"
